        "    title: \"Products\",\n",
        "    IndexDefinition.Create(\"Laptop\", \"Laptop\"),\n",
        "    IndexDefinition.Create(\"Mouse\", \"Mouse\"),\n",
        "    IndexDefinition.Create(\"Keyboard\", \"Keyboard\"))\n",
